<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="iReportLattes_subreport_conferencias" pageWidth="216" pageHeight="200" whenNoDataType="BlankPage" columnWidth="216" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="2a1d649c-140e-473a-9a92-b7f48ad3070a">
	<property name="ireport.zoom" value="3.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="parametroDataInicio" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="parametroDataFim" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT
 (SELECT
            COUNT(b.TipoOrientacao)
        FROM
            lattes.orientation b
        WHERE (CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)) as 'total',
    (SELECT
            COUNT(b.TipoOrientacao)
        FROM
            lattes.orientation b
        WHERE
            b.TipoOrientacao = 'ORIENTACAO_EM_ANDAMENTO_DE_MESTRADO' and (CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)) AS 'TOTAL_O_EM_ANDAMENTO_DE_MESTRADO',
    (SELECT
            COUNT(b.TipoOrientacao)
        FROM
            lattes.orientation b
        WHERE
            b.TipoOrientacao = 'ORIENTACAO_EM_ANDAMENTO_DE_DOUTORADO' and (CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)) AS 'TOTAL_O_EM_ANDAMENTO_DE_DOUTORADO',
    (SELECT
            COUNT(b.TipoOrientacao)
        FROM
            lattes.orientation b
        WHERE
            b.TipoOrientacao = 'ORIENTACOES_CONCLUIDAS_PARA_DOUTORADO' and (CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)) AS 'TOTAL_O_CONCLUIDAS_PARA_DOUTORADO',
    (SELECT
            COUNT(b.TipoOrientacao)
        FROM
            lattes.orientation b
        WHERE
            b.TipoOrientacao = 'ORIENTACOES_CONCLUIDAS_PARA_MESTRADO' and (CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)) AS 'TOTAL_O_CONCLUIDAS_PARA_MESTRADO',
    (SELECT
            COUNT(b.TipoOrientacao)
        FROM
            lattes.orientation b
        WHERE
            b.TipoOrientacao = 'ORIENTACAO_INICIACAO_CIENTIFICA'  and (CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)) AS 'TOTAL_O_INICIACAO_CIENTIFICA'
FROM
    lattes.orientation b
where
(CASE WHEN $P{parametroDataInicio} IS NOT NULL AND $P{parametroDataFim} IS NOT NULL
		THEN b.ano BETWEEN $P{parametroDataInicio} and $P{parametroDataFim} ELSE 1=1 END)
GROUP BY b.TipoOrientacao;]]>
	</queryString>
	<field name="total" class="java.lang.Long"/>
	<field name="TOTAL_O_EM_ANDAMENTO_DE_MESTRADO" class="java.lang.Long"/>
